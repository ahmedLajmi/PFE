												<parameter key="align" value="right" />
											</editor>
										</attribute>
										<attribute name="value" width="80">
											<editor
												classname="de.hybris.platform.hmc.attribute.StringEditorChip">
												<parameter key="align" value="right" />
											</editor>
										</attribute>
									</itemeditorlayout>
								</referencecollectioneditor>
							</attribute>
						</listlayout>
					</section>
					<section name="section.order">
						<listlayout>
							<attribute name="mode" />
							<attribute name="order" />
						</listlayout>
					</section>
					<section name="section.meta">
						<listlayout>
							<attribute name="accept" />
						</listlayout>
					</section>
					<section name="section.text">
						<listlayout>
							<attribute name="textSeperator" />
						</listlayout>
					</section>
					<section name="section.param">
						<listlayout>
							<attribute name="productCode" />
