								<referencecollectioneditor
									editable="true" externalcreate="false" remove="true"
									create="true" autosave="true">
									<itemeditorlayout>
										<attribute name="key" width="77">
											<editor
												classname="de.hybris.platform.hmc.attribute.StringEditorChip">
												<parameter key="align" value="right" />
											</editor>
										</attribute>
										<attribute name="value" width="80">
										</attribute>
									</itemeditorlayout>
								</referencecollectioneditor>
							</attribute>
						</listlayout>
					</section>
					<section name="section.perso">
						<listlayout>
							<attribute name="personalisedParameters">
								<referencecollectioneditor
									editable="true" externalcreate="false" remove="true"
									create="true" autosave="true">
									<itemeditorlayout>
										<attribute name="key" width="77">
											<editor
												classname="de.hybris.platform.hmc.attribute.StringEditorChip">
												<parameter key="align" value="right" />
											</editor>
										</attribute>
										<attribute name="value" width="80">
											<editor
												classname="de.hybris.platform.hmc.attribute.StringEditorChip">
												<parameter key="align" value="right" />
											</editor>
										</attribute>
									</itemeditorlayout>
								</referencecollectioneditor>
							</attribute>
						</listlayout>
					</section>
					<section name="section.response">
						<listlayout>
							<attribute name="priceKey"/>
							<attribute name="currencyKey"/>
							<attribute name="responseTemplate">
								<textareaeditor />
							</attribute>
						</listlayout>
					</section>
				</tab>
			</editor>
		</organizer>
	</type>

